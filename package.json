{
  "name": "@nixinx/core",
  "version": "0.0.6",
  "private": false,
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "dev": "next dev",
    "env:use:nixinx": "node -e \"const fs=require('fs');fs.copyFileSync('.env.nixinx.local','.env.local');console.log('Using .env.nixinx.local -> .env.local');\"",
    "env:use:branch": "powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); Copy-Item -Force ('.env.'+$b+'.local') .env.local; Write-Host ('Using .env.'+$b+'.local -> .env.local')\"",
    "nixinx": "npm run clean && npm run env:use:nixinx && npm install && npm run _check:build && npm run _git:push && next dev -p 3001",
    "nixinx:quick": "npm run env:use:nixinx && npm run _check:build && npm run _git:push && next dev -p 3001",
    "nixinx:branch": "npm run clean && npm install && npm run env:use:branch && npm run _check:build && npm run _git:push && next dev -p 3001",
    "typecheck": "tsc --noEmit",
    "build:lib": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:app": "next build",
    "build": "npm run build:lib",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    ".": "npm install && npm audit fix && next lint && next build && git status && git add . && (git commit -m \"General Adjustments\" || echo No changes) && powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); git push -u origin $b\" && cls && next dev",
    "all": "npm install && npm audit fix && next lint && next build && next postbuild && next start",
    "git": "git status && git add . && (git commit -m \"General Adjustments\" || echo No changes) && powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); git push -u origin $b\" && cls && next dev",
    "dev:nixinx": "npm run env:use:nixinx && next dev -p 3001",
    "dev:elpatron": "node -e \"const fs=require('fs');fs.copyFileSync('.env.elpatronbarandgrill.local','.env.local');console.log('Using .env.elpatronbarandgrill.local -> .env.local');\" && next dev -p 3002",
    "dev:ht-windsor": "node -e \"const fs=require('fs');fs.copyFileSync('.env.hottacoswindsor.local','.env.local');console.log('Using .env.hottacoswindsor.local -> .env.local');\" && next dev -p 3003",
    "dev:ht-leamington": "node -e \"const fs=require('fs');fs.copyFileSync('.env.hottacosleamington.local','.env.local');console.log('Using .env.hottacosleamington.local -> .env.local');\" && next dev -p 3004",
    "_check:build": "npm run typecheck && npm run lint && next build && next-sitemap",
    "_git:push": "powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); git status; git add -A; git commit -m 'General Adjustments' 2>$null; git push -u origin $b\"",
    "clean": "node -e \"const fs=require('fs');for(const p of ['.next','.turbo','node_modules','dist']){try{fs.rmSync(p,{recursive:true,force:true});console.log('removed',p);}catch{}}\"",
    "clean:deep": "node -e \"const fs=require('fs');for(const p of ['.next','.turbo','node_modules','dist']){try{fs.rmSync(p,{recursive:true,force:true});console.log('removed',p);}catch{}}\"",
    "build:pkg": "tsc -p tsconfig.pkg.json",
    "publish:nixinx": "npm run clean:deep && npm install && npm run build:pkg && npm publish --access=restricted",
    "deploy:nixinx": "npm run clean:deep && npm install && npm run env:use:nixinx && npm run typecheck && npm run lint && npm run build:pkg && npm run _git:push && npm publish --access=restricted && npm run dev:nixinx"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./wrappers": {
      "types": "./dist/complements/components/ui/wrappers/index.d.ts",
      "default": "./dist/complements/components/ui/wrappers/index.js"
    },
    "./StyleDesigner": {
      "types": "./dist/complements/components/StyleDesigner/StyleDesigner.d.ts",
      "default": "./dist/complements/components/StyleDesigner/StyleDesigner.js"
    },
    "./admin/BrandingTab": {
      "types": "./dist/complements/admin/BrandingTab.d.ts",
      "default": "./dist/complements/admin/BrandingTab.js"
    },
    "./admin/EnvWizard": {
      "types": "./dist/complements/admin/EnvWizard.d.ts",
      "default": "./dist/complements/admin/EnvWizard.js"
    },
    "./admin/FMsTab": {
      "types": "./dist/complements/admin/FMsTab.d.ts",
      "default": "./dist/complements/admin/FMsTab.js"
    },
    "./admin/MetaTab": {
      "types": "./dist/complements/admin/MetaTab.d.ts",
      "default": "./dist/complements/admin/MetaTab.js"
    },
    "./seeds/styles": {
      "types": "./dist/seeds/styles.d.ts",
      "default": "./dist/seeds/styles.js"
    },
    "./ui/providers": {
      "types": "./dist/app/ui/providers.d.ts",
      "default": "./dist/app/ui/providers.js"
    },
    "./coreinterfaces": {
      "types": "./dist/coreinterfaces.d.ts",
      "default": "./dist/coreinterfaces.js"
    },
    "./styles/core-globals.css": "./dist/app/globals.css"
  },
  "sideEffects": false,
  "dependencies": {
    "@capacitor/core": "^7.4.4",
    "@ducanh2912/next-pwa": "^10.2.4",
    "@nixinx/core": "^0.0.3",
    "@paypal/react-paypal-js": "^8.8.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@react-google-maps/api": "^2.20.7",
    "@stripe/stripe-js": "^7.9.0",
    "@vercel/analytics": "^1.5.0",
    "ai": "^5.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.5.0",
    "lucide-react": "^0.544.0",
    "mapbox-gl": "^3.14.0",
    "next": "^15.4.2",
    "next-pwa": "^5.6.0",
    "openai": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.14.1",
    "react-share": "^5.0.3",
    "react-social-icons": "^6.24.0",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "undici": "^7.12.0"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.8.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.4",
    "@emailjs/browser": "^3.10.0",
    "@types/estree": "^1.0.8",
    "@types/google.maps": "^3.58.1",
    "@types/gtag.js": "^0.0.18",
    "@types/minimatch": "^6.0.0",
    "@types/next": "^9.0.0",
    "@types/node": "^20.19.9",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-share": "^3.0.3",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "react-intl": "^6.6.8",
    "tailwindcss": "^3.3.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5",
    "webpack": "^5.90.0"
  }
}
