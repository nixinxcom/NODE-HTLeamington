{
  "name": "nixinx",
  "version": "0.1.0",
  "private": true,
"scripts": {
  "dev": "next dev",

  "env:use:nixinx": "node -e \"const fs=require('fs');fs.copyFileSync('.env.nixinx.local','.env.local');console.log('Using .env.nixinx.local -> .env.local');\"",
  "env:use:branch": "powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); Copy-Item -Force ('.env.'+$b+'.local') .env.local; Write-Host ('Using .env.'+$b+'.local -> .env.local')\"",

  "nixinx": "npm run clean && npm run env:use:nixinx && npm ci && npm run _check:build && npm run _git:push && next dev -p 3001",

  "nixinx:quick": "npm run env:use:nixinx && npm run _check:build && npm run _git:push && next dev -p 3001",

  "nixinx:branch": "npm run clean && npm ci && npm run env:use:branch && npm run _check:build && npm run _git:push && next dev -p 3001",

  "typecheck": "tsc --noEmit",
  "build": "next build",
  "postbuild": "next-sitemap",
  "start": "next start",
  "lint": "next lint",

  ".": "npm install && npm audit fix && next lint && next build && git status && git add . && (git commit -m \"General Adjustments\" || echo No changes) && powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); git push -u origin $b\" && cls && next dev",
  "all": "npm install && npm audit fix && next lint && next build && next postbuild && next start",
  "git": "git status && git add . && (git commit -m \"General Adjustments\" || echo No changes) && powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); git push -u origin $b\" && cls && next dev",

  "dev:nixinx": "npm run env:use:nixinx && next dev -p 3001",
  "dev:elpatron": "node -e \"const fs=require('fs');fs.copyFileSync('.env.elpatronbarandgrill.local','.env.local');console.log('Using .env.elpatronbarandgrill.local -> .env.local');\" && next dev -p 3002",
  "dev:ht-windsor": "node -e \"const fs=require('fs');fs.copyFileSync('.env.hottacoswindsor.local','.env.local');console.log('Using .env.hottacoswindsor.local -> .env.local');\" && next dev -p 3003",
  "dev:ht-leamington": "node -e \"const fs=require('fs');fs.copyFileSync('.env.hottacosleamington.local','.env.local');console.log('Using .env.hottacosleamington.local -> .env.local');\" && next dev -p 3004",

  "_check:build": "npm run typecheck && npm run lint && next build && next-sitemap",

  "_git:push": "powershell -NoProfile -Command \"$b=(git rev-parse --abbrev-ref HEAD); git status; git add -A; git commit -m 'General Adjustments' 2>$null; git push -u origin $b\"",

  "clean": "node -e \"const fs=require('fs');for(const p of ['.next','.turbo','node_modules']){try{fs.rmSync(p,{recursive:true,force:true});console.log('removed',p);}catch{}}\"",
  "clean:deep": "node -e \"const fs=require('fs');for(const p of ['.next','.turbo','node_modules','package-lock.json']){try{fs.rmSync(p,{recursive:true,force:true});console.log('removed',p);}catch{}}\""
},
  "dependencies": {
    "@capacitor/core": "^7.4.4",
    "@ducanh2912/next-pwa": "^10.2.4",
    "@paypal/react-paypal-js": "^8.8.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@react-google-maps/api": "^2.20.7",
    "@stripe/stripe-js": "^7.9.0",
    "@vercel/analytics": "^1.5.0",
    "ai": "^5.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.5.0",
    "lucide-react": "^0.544.0",
    "mapbox-gl": "^3.14.0",
    "next": "^15.4.2",
    "next-pwa": "^5.6.0",
    "openai": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.14.1",
    "react-share": "^5.0.3",
    "react-social-icons": "^6.24.0",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "undici": "^7.12.0"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.8.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.4",
    "@emailjs/browser": "^3.10.0",
    "@types/estree": "^1.0.8",
    "@types/google.maps": "^3.58.1",
    "@types/gtag.js": "^0.0.18",
    "@types/minimatch": "^6.0.0",
    "@types/next": "^9.0.0",
    "@types/node": "^20.19.9",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-share": "^3.0.3",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "react-intl": "^6.6.8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "webpack": "^5.90.0"
  }
}
